{
  "version": 2,
  "builds":[
    {"src":"api/server.js","use":"@vercel/node"},
    {"src":"frontend/index.html","use":"@vercel/static-build","config":{"distDir":"frontend"}}
  ],
  "routes":[
    {"src":"/api/(.*)","dest":"api/server.js"},
    {"handle":"filesystem"},
    {"src":"/(.*)","dest":"frontend/index.html"}
  ]
}
